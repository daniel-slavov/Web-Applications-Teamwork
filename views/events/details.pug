extends ../layout

block content
    h1#title
        | #{event.title}
    if user && user.username == event.user
        button(class="btn btn-danger") Delete event
    div#details.col-md-7
        img(src=event.photo)
        br

    div#chat.col-md-5
        ul#messages
            each message in chat
                li.media
                    .media-body  
                        a.pull-left(href='#')
                            img.media-object.img-circle(src='data:image/jpeg;base64,')
                        |#{message.content}
                        br
                        small.text-muted Posted by #{message.user} at #{message.time}
                        hr
        div#input.col-lg-12
            input#new-message.col-lg-10.form-control(autocomplete='off', placeholder="Enter your message...")
            button#send.col-lg-2.btn.btn-success Send
    script(src='/socket.io/socket.io.js')
    script(src='https://code.jquery.com/jquery-1.11.1.js')
    script.
        $(function () {
        if(!document.getElementById('username')) {
            $('#input').hide();
        }
        var socket = io();
        $('#send').click(function () {
        socket.emit('chat message', {user: $('#username').text(),content: $('#new-message').val(), time: (new Date).toLocaleTimeString(), room: $('#title').text()});
        $('#new-message').val('');
        return false;
        });
        socket.on('chat message', function (msg) {
        $('#messages').append($(`<li class="media"><div class="media-body"><a href="#" class="pull-left"><img class="media-object img-circle" src="data:image/jpeg;base64,"></a>${msg.content}<br><small class="text-muted">Posted by ${msg.user} at ${msg.time}</small><hr></div></li>`));
        });
        });